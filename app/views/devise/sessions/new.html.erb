<span id="page-content-start"></span>

<section class="normal-section">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <ol class="breadcrumb">
          <li><a href="">Home</a></li>
          <li class="about-person.html">Log In</li>
        </ol>
      </div>
      <div class="col-md-4 col-sm-4 col-xs-12">
        <h2>Log In</h2>
        <p>
          Don't have an account?<br>
          <%= render "devise/shared/links" %>
        </p>
      </div>
      <div class="col-md-8 col-xs-12">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>
        <%= f.email_field :email, autofocus: true, class: ['text-box-outline', 'wow', 'fadeInUp', 'fill-width'], placeholder: "Email" %>
        <%= f.password_field :password, autocomplete: "off", class: ['text-box-outline', 'wow', 'fadeInUp', 'fill-width'], placeholder: "Password" %>
        <%= f.submit "Log in", class: ['btn-outline-white','wow','fadeInUp','fill-width'] %>
        <% if devise_mapping.rememberable? -%>
        <p><%= f.check_box :remember_me %>
          <%= f.label :remember_me %></p>
          <% end -%>
          <% end %>
        </div>
        
      </div>
    </div>
  </section>